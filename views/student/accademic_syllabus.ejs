<div class="col-lg-12 grid-margin stretch-card">
	<div class="card">
		<div class="card-body">
			<h2 class="card-title"><b><u>Accademic Syllabus</u></b></h2>
			<p class="card-description">
				<a href="#"><button class="btn btn-inverse-dark btn-fw">Accademic Syllabus</button></a>
				
			</p>
			
			<div id="study" >
				<h2>Study Material</h2>
				<div class="table-responsive">
					<table class="table table-bordered" id="study_material">
						<thead>
							<tr>
								<th>
									#
								</th>
								
								<th>
									Class Name
								</th>	
								
								<th>
									Subject Name 
								</th>
								<th>
									Title 
								</th>
								<th>
									Description
								</th>
								<th>File</th>
								
								
							</tr>
						</thead>
						<tbody>
							<%  var i=1; accademic_syllabus.forEach(function(data){ 
						
					%>
						<tr id="tr_<%= i %>">
							<td >

								<%= i %>
							</td>
							<td>
								<%= data.class_name %>
							</td>
							<td>
								<%= data.subject_name %>
							</td>
							<td>
								<%= data.title %>
							</td>
							<td>
								<%= data.description %>
							</td>
							<td><a href="./../../images/<%= data.file_name %>" download>Download</a></td>
						</tr>
					<% i++;  })  %>
						</tbody>
					</table>
				</div>
			</div>
					
		</div>
	</div>
</div>
