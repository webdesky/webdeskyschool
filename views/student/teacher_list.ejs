<div class="col-lg-12 grid-margin stretch-card">
	<div class="card">
		<div class="card-body">
			<h2 class="card-title"><b><u>Teacher List</u></b></h2>
			<p class="card-description">
				<a href="#"><button class="btn btn-inverse-dark btn-fw">Teachers</button></a>
				
			</p>
			<div class="table-responsive">
				<table class="table table-bordered" id="myTable">
					<thead>
						<tr>
							<th>
								#
							</th>
							<th>
								Name 
							</th>
							<th>
								Email
							</th>	
							
							
							
						</tr>
					</thead>
					<tbody>
					<%  var i=1; teacher_list.forEach(function(data){ 
						
					%>
						<tr id="tr_<%= i %>">
							<td >

								<%= i %>
							</td>
							<td>
								<%= data.name %>
							</td>
							<td>
								<%= data.email %>
							</td>
							
						</tr>
					<% i++;  })  %>
					</tbody>
				</table>
			</div>


					
		</div>
	</div>
</div>
