<div class="col-lg-12 grid-margin stretch-card">
	<div class="card">
		<div class="card-body">
			<h2 class="card-title"><b><u>Dormitory List</u></b></h2>
			<p class="card-description">
				<a href="<%= appName %>/admin/dormitory"><button class="btn btn-inverse-dark btn-fw">Add Dormitory</button></a>
				
			</p>
			<div class="table-responsive">
				<table class="table table-bordered datatable">
					<thead>
						<tr>
							<th>
								#
							</th>
							<th>
								Name
							</th>
							<th>
								Number Of Room
							</th>
							<th>
								Description
							</th>
							<th>
								Action
							</th>
							
						</tr>
					</thead>
					<tbody>
						<%  var i=1; dormitory_list.forEach(function(dormitory_list){ %>
						<tr id="tr_<%= i %>">
							<td>
								<%= i %>
							</td>
							<td>
								<%= dormitory_list.name %>
							</td>
							<td>
								<%= dormitory_list.number_of_room %>
							</td>
							
							<td>
								<%= dormitory_list.description %>
							</td>
							<td>
							 <a href="<%= appName %>/admin/dormitory/?id=<%= dormitory_list.dormitory_id %>">
								<button type="button" class="btn btn-dark btn-fw">
                         	 	<i class="mdi mdi-cloud-download"></i>Edit
                         	 	</button>
                         	 </a>
                         	 <a href="#"  onclick="delete_record('<%= dormitory_list.dormitory_id %>','<%= i %>','tbl_dormitory','dormitory_id')" class="btn btn-sm btn-danger">Delete</a>
							</td>
						</tr>
						<% i++; })%>
					</tbody>
				</table>
			</div>
		</div>
	</div>
</div>