			               	 <option value="<%= routinedata.section_id %>"><%= routinedata.section_name %></option>
			               	<% } %>
			                 <option>Select Section</option>
			               </select>
			              </div>

			              <div class="col-sm-3">
			               	<button class="btn btn-primary" value="Submit" >Search</button>

			              </div>
			        </form>
			        <br><br><br>

			        <% if(class_routine!=''){ %>
					<div class="table-responsive">
						<table class="table table-bordered" id="myTable">
							<thead>
								<tr>
									<th>
										Day
									</th>
									<th>
										Routine
									</th>
								</tr>
							</thead>
							<tbody>
							<% Object.keys(class_routine).forEach(function (key) { %>
								<tr>
								<td><%= key %></td>
								 
								<td>
						    		<%  Object.keys(class_routine[key]).forEach(function (skey){ %>
						    		<%=  class_routine[key][skey].subject_name %> 
						    		<span>(</span>
						    		<%= class_routine[key][skey].time_start  %>
						    		<span>:</span>
						    		<%= class_routine[key][skey].time_start_min  %>
						    		<% if(class_routine[key][skey].starting_ampm==1){ %>
						    		<span>am</span>
						    		<% }else{ %>
						    		<span>pm</span>
						    		<% } %>
						    		<span>-</span>
						    		<%= class_routine[key][skey].time_end  %>
						    		<span>:</span>
						    		<%= class_routine[key][skey].time_end_min  %>

						    		<% if(class_routine[key][skey].end_ampm==1){ %>
						    		<span>am</span>
						    		<% }else{ %>
						    		<span>pm</span>
						    		<% } %>
						    		<span>)</span>
						    		<% }); %>
						    		
						    	</td>
									<table>
								
							</table>

								</td>
								</tr>
						      				  
						   <% }); %>
							</tbody>
						</table>
					</div>
					<% } %>
			</div>
		</div>
</div>