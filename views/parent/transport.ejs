<div class="col-lg-12 grid-margin stretch-card">
	<div class="card">
		<div class="card-body">
			<h2 class="card-title"><b><u>Transport List</u></b></h2>
			<p class="card-description">
				<a href="/admin/AssignTeacher"><button class="btn btn-inverse-dark btn-fw">Transport Taken</button></a>
				
			</p>
			<div class="table-responsive">
				<table class="table table-bordered" id="myTable">
					<thead>
						<tr>
							<th>
								#
							</th>
							<th>
								Name 
							</th>
							<th>
								Class Name
							</th>	
							<th>Section Name</th>
							
							<th>
								Route 
							</th>
							<th>
								Route Fare
							</th>
							
							
							
						</tr>
					</thead>
					<tbody>
					<%  var i=1; transport.forEach(function(data){ %>
						<tr id="tr_<%= i %>">
							<td >

								<%= i %>
							</td>
							<td>
								<%= data.name %>
							</td>
							<td>
								<%= data.class_name %>
							</td>
							<td>
								<%= data.section_name %>
							</td>
							<td><%= data.route_name %></td>
							<td><%= data.route_fare %></td>
							
							

							
						</tr>
					<% i++; })%>
					</tbody>
				</table>
			</div>


					
		</div>
	</div>
</div>
