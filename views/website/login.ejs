	<!-- /services-home -->
	<div class="programs py-5" id="Events">
		<div class="w3-head-all mb-3">
		 <h3>Login</h3>
		</div>
		<div class="container grids_sec_2">
			<div class="row style-grids_main">



				<div class="col-lg-4 mx-auto">
            <div class="auto-form-wrapper">

           <div class="container">
             
              <% if(success!=""){ %>
              <div class="alert alert-success" id="success-alert">
                <button type="button" class="close" data-dismiss="alert">x</button>
                <strong><%= success %> </strong>
                
              </div>
                
             <% 
            } %>
            <% if(error!=""){ %>
              
               <div class="alert alert-danger" id="danger-alert">

                
                <button type="button" class="close" data-dismiss="alert">x</button>
                <strong><%= error %> </strong>
                
              </div>
           <% }  %>
          </div>

          
            <form action="<%= appName %>/admin/dashboard" method="post" enctype="multipart/form-data">
            
                <div class="form-group">
                  <label class="label">Email</label>
                  <div class="input-group">
                    <input type="text" class="form-control" placeholder="Username" name="username">
                    
                  </div>
                </div>
                <div class="form-group">
                  <label class="label">Password</label>
                  <div class="input-group">
                    <input type="password" class="form-control" placeholder="*********" name="password">
                   
                  </div>
                </div>
                <div class="form-group">
                  <button style="background-color: #6f6f6f; border-color: #ffae11;" class="btn btn-primary submit-btn btn-block">Login</button>
                </div>
                <div class="form-group d-flex justify-content-between">
                  
                  <a href="/forgot_password" class="text-small forgot-password text-black" style="color: #ffae11">Forgot Password</a>
                </div>
               
              </form>
            </div>
            
          </div>

			 

			</div>
		</div>
	</div>
	<!-- //services-home -->

 