<div class="col-lg-12 grid-margin stretch-card">
	<div class="card">
		<div class="card-body">
			<h2 class="card-title"><b><u>Study Material</u></b></h2>
			<p class="card-description">
				<a href="#"><button class="btn btn-inverse-dark btn-fw">Study Material</button></a>
				
			</p>
			<div class="table-responsive">
				<table class="table table-bordered" id="myTable">
					<thead>
						<tr>
							<th>
								#
							</th>
							<th>
								Name 
							</th>
							<th>
								Class Name
							</th>	
							<th>Section Name</th>
							<th>
								Dob 
							</th>
							<th>
								Gender 
							</th>
							<th>
								Email
							</th>
							<th>Phone</th>
							<th>Action</th>
							
						</tr>
					</thead>
					<tbody>
					<%  var i=1; student_information.forEach(function(data){ %>
						<tr id="tr_<%= i %>">
							<td >

								<%= i %>
							</td>
							<td>
								<%= data.name %>
							</td>
							<td>
								<%= data.class_name %>
							</td>
							<td>
								<%= data.section_name %>
							</td>
							<td>
								<%= data.dob %>
							</td>
							
							<td>
								<%= data.sex %>
							</td>
							<td>
								<%= data.email %> 
							</td>
							<td>
								<%= data.phone %>
							</td>
							<td>
								<a href="#"  onclick="get_study_material('<%= data.class_id  %>')" class="btn btn-sm btn-primary">View</a>
							</td>
							

							
						</tr>
					<% i++; })%>
					</tbody>
				</table>
			</div>
			<br><br>
			<div id="study" style="display: none;">
				<h2>Study Material</h2>
				<div class="table-responsive">
					<table class="table table-bordered" id="study_material">
						<thead>
							<tr>
								<th>
									#
								</th>
								
								<th>
									Class Name
								</th>	
								
								<th>
									Subject Name 
								</th>
								<th>
									Title 
								</th>
								<th>
									Description
								</th>
								<th>File</th>
								
								
							</tr>
						</thead>
						<tbody>
						
						</tbody>
					</table>
				</div>
			</div>
					
		</div>
	</div>
</div>
